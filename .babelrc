{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": "last 1 electron version",
        "loose": true,
        "modules": false,
        "useBuiltIns": "entry",
        "corejs": 3
      }
    ],
    "@babel/preset-react"
  ],
  "plugins": [
    "@knoopx/jsx-classnames",
    "@babel/plugin-proposal-export-default-from"
  ],
  "env": {
    "development": {
      "plugins": [
        "react-hot-loader/babel"
      ]
    }
  }
}
